Algoritmo Medicina
	Escribir Mater, Trauma, Pedi, M;
	Escribir "El monto total es: ";
	Leer M;
	Si (M>2000000) Entonces
		Mater=M*(1-0.25);
		Trauma=M*(1-0.35);
		Pedi=M*(1-0.25);
	Sino
		Mater=M*(1-0.3);
		Trauma=M*(1-0.4);
		Pedi=M*(1-0.3);
	Fin Si
	Escribir "El monto para maternidad es: ";
	Escribir Mater;
	Escribir "El monto para traumatologia es: ";
	Escribir Trauma;
	Escribir "El monto para pediatria es: ";
	Escribir Pedi;
FinAlgoritmo
